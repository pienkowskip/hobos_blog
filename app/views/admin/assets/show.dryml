<show-page for="Asset">
  <prepend-content-body:><%=
    link_to(this.asset.url) do
      (this.asset.exists?(:medium) ? image_tag(this.asset.url(:medium)) : this.asset_file_name)
    end
  %></prepend-content-body:>
  <field-list: fields="id, name, asset_file_size, asset_content_type, asset_updated_at" />
</show-page>
